implement char {
    template <TargetType>
    func cast()->TargetType{
        return __cast<char, TargetType>(this);
    }
}


implement operator::Equal<char> for char {
    @instruction("ICmp", "ICMP_EQ")
    func Equal(operand: char)->bool;
}

implement operator::NotEqual<char> for char {
    @instruction("ICmp", "ICMP_NE")
    func NotEqual(operand: char)->bool;
}


